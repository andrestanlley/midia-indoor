generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Terminal {
  deviceId    String     @id
  name        String
  lastSync    DateTime
  midiaListId String?
  MidiaList   MidiaList? @relation(fields: [midiaListId], references: [id])
}

model MidiaList {
  id       String     @id
  name     String
  midias   Midia[]
  Terminal Terminal[]
}

model Midia {
  id          String     @id
  filename    String
  type        String
  midiaListId String?
  MidiaList   MidiaList? @relation(fields: [midiaListId], references: [id])
}
